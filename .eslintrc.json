{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "strict": "off",
        "no-console": "off",
        "import/no-unresolved": "off",
        "class-methods-use-this": "off",
        "no-unused-vars": "off"
    }
}
